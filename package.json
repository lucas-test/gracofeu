{
    "name": "gracofeu",
    "version": "1.0.0",
    "description": "",
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "scripts": {
        "clean": "rimraf dist/*",
        "gen-param-doc": "run-for-every-file --src \"./src/public/parametors/parametors_info/\" --dest \"./src/public/parametors/parametors_info/\" --file \"*.md\" --run \"marked  {{src-file}} -o {{dest}}{{file-name}}.html\"",
        "build": "npm run gen-param-doc && tsc && shx cp -r src/public/img dist/public/ && parcel build src/public/index.html --no-optimize --dist-dir dist/public",
        "build-client": "npm run gen-param-doc && npm run clean  && parcel build src/public/index.html --no-optimize --dist-dir dist/public && shx cp -r src/public/img dist/public/",
        "start": "node dist/server/app.js",
        "dev": " npm run clean && npm run build && npm start",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lucas-test/gracofeu.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/lucas-test/gracofeu/issues"
    },
    "homepage": "https://github.com/lucas-test/gracofeu#readme",
    "devDependencies": {
        "@types/express": "^4.17.13",
        "buffer": "^6.0.3",
        "marked": "^4.0.19",
        "run-for-every-file": "^1.1.0",
        "shx": "^0.3.4",
        "tslint": "^6.1.3",
        "typescript": "^4.7.2"
    },
    "dependencies": {
        "express": "^4.18.1",
        "parcel": "^2.6.0",
        "rimraf": "^3.0.2",
        "socket.io": "^4.5.1",
        "socket.io-client": "^4.5.1"
    }
}